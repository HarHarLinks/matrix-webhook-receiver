<strong>{{ title }}</strong><br>
{{ message }}<br>
<a href="{{ ruleUrl }}">{{ ruleName }}</a>: {{ state }}<br>
<ul>
{% for e in evalMatches %}
<li>{{ e.metric }}: {{ e.value }}{% if e.tags %} ({% for key, value in e.tags.items() %}{{ key|e }}: {{ value|e }},{% endfor %}){% endif %}</li>
{% endfor %}
</ul>
